{
  "openapi": "3.1.0",
  "info": {
    "title": "Petstore Primitives",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://petstore.example.com/api/v1"
    }
  ],
  "paths": {
    "/status": {
      "get": {
        "tags": ["Status"],
        "operationId": "getStatus",
        "summary": "Get health status",
        "responses": {
          "200": {
            "description": "Health check string",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/status/code": {
      "get": {
        "tags": ["Status"],
        "operationId": "getStatusCode",
        "summary": "Get numeric status code",
        "responses": {
          "200": {
            "description": "Numeric status code",
            "content": {
              "application/json": {
                "schema": {
                  "type": "integer"
                }
              }
            }
          }
        }
      }
    },
    "/status/healthy": {
      "get": {
        "tags": ["Status"],
        "operationId": "isHealthy",
        "summary": "Get boolean health flag",
        "responses": {
          "200": {
            "description": "Boolean health flag",
            "content": {
              "application/json": {
                "schema": {
                  "type": "boolean"
                }
              }
            }
          }
        }
      }
    },
    "/status/tags": {
      "get": {
        "tags": ["Status"],
        "operationId": "getStatusTags",
        "summary": "Get list of status tags",
        "responses": {
          "200": {
            "description": "List of tags",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/status/reset": {
      "post": {
        "tags": ["Status"],
        "operationId": "resetStatus",
        "summary": "Reset status",
        "responses": {
          "204": {
            "description": "Status reset successfully"
          }
        }
      }
    }
  }
}
